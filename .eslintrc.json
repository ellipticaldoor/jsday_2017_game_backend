{
	"env": {
		"es6": true,
		"node": true,
		"mocha": true
	},
	"extends": "standard",
	"rules": {
		"arrow-parens": 0,
		"generator-star-spacing": 0,
		"no-tabs": 0,
		"indent": ["error", "tab"],
		"space-before-function-paren": 0,
		"camelcase": 0,
		"no-console": 0,
		"no-unused-vars": 1,
		"comma-dangle": [
			"error",
			{
				"arrays": "always-multiline",
				"objects": "always-multiline",
				"imports": "always-multiline",
				"exports": "always-multiline",
				"functions": "ignore"
			}
		]
	}
}
